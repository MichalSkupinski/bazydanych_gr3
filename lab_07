select nazwa, idkreatury, nazwa, idkreatury from kreatura;
select*from kreatura kreaturak
select nazwa, idkreatury, nazwa, idkreatury from kreatura;
select*from kreatura kreaturak
inner join kreatura k2 on k.idkreatury k2.idkreatury;
select * from kreatura k 
inner join ekwipunek e on k.idkreatury=e.idkreatury
select k.rodzaj avg(e,ilosc*z.waga)
from kreatura k2
inner join ekwipunek e on k.idkreatury=e.id kreatury
inner join zasob z on e.idzasobu=z.idzasobu
where k.rodzaj not in(MALPA,WAZ)--and e.ilosz< 30
group by k.rodzaj having sum(e.ilosc)<30;
