select k.nazwa from kreatura.k;
select rodaj, min (dataUr) AS MIN, MAX(DATAUR) as max from kreatura.k group by rodzaj) as pod
where k.dataUr=pod.min and k.rodzaj=pod.rodzaj;
select nazwa , rodzaj , min(dataur) from kreatura group by rodzaj 


Create table kreatura select * from wikingowie.kreatura;

Create table zasob select * from wikingowie.zasob;

Create table ekwipunek select * from wikingowie.ekwipunek;

select * from zasob;

select * from zasob where rodzaj = "jedzenie";

select idZasobu, nazwa from zasob where idZasobu IN(3,5,7);



select * from kreatura k2left join uczestnicy u on k.idkreatury=u.id_uczestnika
where u.id_uczestnika is null;
select idkreatury from kreatura where idkreatury not int


select nazwa from kreatura  where idkreaturynot in (select dinict id_uczestnika from uczestnicy)
select u.id_uczestnika , k.idkreatury from uzestnicy u right join kreatura k on on u.id_uczestnika=k.ikkreatury
from wyprawa w
inner join uczestnicy u on w.id_wyprawy=u.id_wyprawyinner join ekwipunek e on u.id_uczestnika=e.idkreatury;
inner join zasob z on.eidzasobu=z.idzasobu
group by w id_kreatury;
